<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.meetu.model">

	<class name="FriendReq" table="u_friends_req" dynamic-update="true">
        <composite-id><!--联合主键，表中的主键为(userId,friendId)-->  
        <!--name及cardID为Student类中的属性-->
           <key-property name="userId" column="userId" type="java.lang.String"></key-property>  
           <key-property name="friendId" column="friendId" type="java.lang.String"></key-property>  
        </composite-id>  
		<property  name="userId" column="userId" type="java.lang.Integer" />
		<property name="friendId" column="friendId" type="java.lang.Integer" />
		<property name="statusRel" column="statusRel" type="java.lang.String" />
		<property name="happenTime" column="happenTime" type="java.lang.String" />
	</class>

</hibernate-mapping>

