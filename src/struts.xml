<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<constant name="struts.devMode" value="true" />
	<!-- <constant name="struts.multipart.saveDir" value="/tmpUploadFileFolder"/> -->
	
	<!-- 设置编码格式 -->
 	<constant name="struts.i18n.encoding" value="UTF-8"></constant>
	
	<package name="forward" extends="struts-default" namespace="/">
		<action name="index">
			<result>/index.jsp</result>
		</action>
	</package>


	<package name="user" extends="struts-default" namespace="/">
		<action name="userAction" class="org.meetu.action.UserAction">
		</action>
	</package>

	<package name="pub" extends="struts-default" namespace="/">
		<action name="pubAction" class="org.meetu.action.PubAction">
		</action>
	</package>
	
	<package name="meetu" extends="struts-default" namespace="/">
		<action name="meetuAction" class="org.meetu.action.MeetuAction">
		</action>
	</package>
	
	<package name="friend" extends="struts-default" namespace="/">
		<action name="friendAction" class="org.meetu.action.FriendAction">
		</action>
	</package>
	
	<package name="feedbak" extends="struts-default" namespace="/">
		<action name="feedbackAction" class="org.meetu.action.FeedbackAction">
		</action>
	</package>	
	
	<package name="device" extends="struts-default" namespace="/">
		<action name="deviceAction" class="org.meetu.action.DeviceAction">
		</action>
	</package>		
	
	<package name="help" extends="struts-default" namespace="/">
		<action name="helpAction" class="org.meetu.action.CallForHelpAction">
		</action>
	</package>		
	
	
	<package name="default" extends="struts-default">
	   <default-action-ref name="notFound" />
	   <action name="notFound">
	   	<result>/error/404.jsp</result>
	   </action>
	</package>
</struts>
